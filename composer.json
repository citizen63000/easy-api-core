{
  "name": "citizen63000/easy-api-core",
  "type": "symfony-bundle",
  "description": "",
  "keywords": ["api", "rest", "symfony"],
  "require": {
    "php": ">=8.1",
    "symfony/console": "^5.4",
    "symfony/serializer": "^5.4",
    "doctrine/doctrine-bundle": "^2.7",
    "ramsey/uuid-doctrine": "^1.8",
    "fakerphp/faker": "v1.23.1"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "@stable"
  },
  "config": {
    "discard-changes": true,
    "platform": {
      "php": "8.1"
    },
    "sort-packages": true,
    "allow-plugins": {
      "symfony/flex": true
    }
  },
  "autoload": {
    "psr-4": {
      "EasyApiCore\\": ""
    }
  }
}
