{
  "name": "citizen63000/easy-api-core",
  "type": "symfony-bundle",
  "description": "",
  "keywords": ["api", "rest", "symfony"],
  "require": {
    "php": ">=8.3",
    "symfony/console": "^6.4",
    "symfony/serializer": "^6.4",
    "symfony/form": "^6.4",
    "symfony/validator": "^6.4",
    "doctrine/doctrine-bundle": "^2.7",
    "doctrine/orm": "^2.7",
    "ramsey/uuid-doctrine": "^2.0",
    "fakerphp/faker": "v1.23.1",
    "nelmio/api-doc-bundle": "4.10.1"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "@stable"
  },
  "config": {
    "discard-changes": true,
    "platform": {
      "php": "8.3"
    },
    "sort-packages": true,
    "allow-plugins": {
      "symfony/flex": true
    }
  },
  "autoload": {
    "psr-4": {
      "EasyApiCore\\": "/Framework"
    }
  }
}
